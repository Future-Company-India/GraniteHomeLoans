{
    "name": "futurecompany/graniteloan",
    "type": "project",
    "license": "MIT",
    "description": "WordPress boilerplate with Composer, easier configuration, and an improved folder structure",
    "homepage": "https://roots.io/bedrock/",
    "authors": [
      {
        "name": "Project Development",
        "email": "projectdevelopment@futurecompany.in",
        "homepage": "https://github.com/dev-futureCompany"
      }
    ],
    "keywords": [
      "composer",
      "wordpress",
      "wp",
      "wp-config"
    ],
    "repositories": {
      "0": {
        "type": "composer",
        "url": "https://wpackagist.org",
        "only": [
          "wpackagist-plugin/*",
          "wpackagist-theme/*"
        ]
      }
    },
    "require": {
      "johnpbloch/wordpress": "6.8.2",
      "composer/installers": "^1.0",
      "wpackagist-plugin/better-search-replace": "^1.4",
      "wpackagist-plugin/contact-form-7": "^6.1",
      "wpackagist-plugin/contact-form-cfdb7": "^1.3",
      "wpackagist-plugin/contact-form-7-honeypot": "^3.0",
      "wpackagist-plugin/drag-and-drop-multiple-file-upload-contact-form-7": "^1.3",
      "wpackagist-plugin/ga-google-analytics": "^20250326",
      "wpackagist-plugin/genesis-blocks": "^3.1",
      "wpackagist-plugin/leadin": "^11.3",
      "wpackagist-plugin/jetpack": "^14.9",
      "wpackagist-plugin/mailpoet": "^5.14",
      "wpackagist-plugin/makewebbetter-hubspot-for-woocommerce": "^1.6",
      "wpackagist-plugin/wpcf7-recaptcha": "^1.4",
      "wpackagist-plugin/user-role-editor": "^4.64",
      "wpackagist-plugin/woocommerce": "^10.1",
      "wpackagist-plugin/woocommerce-pos": "^1.7",
      "wpackagist-plugin/woocommerce-gateway-stripe": "^9.8",
      "wpackagist-plugin/woocommerce-services": "^3.0",
      "wpackagist-plugin/woocommerce-payments": "^9.9",
      "wpackagist-plugin/wp-crontrol": "^1.19",
      "wpackagist-plugin/wp-mail-logging": "^1.14",
      "wpackagist-plugin/wp-mail-smtp": "^4.6",
      "wpackagist-plugin/insert-headers-and-footers": "^2.3",
      "wpackagist-plugin/wordpress-seo": "^25.8",
      "wpackagist-theme/twentytwentyone": "^2.6",
      "wpackagist-plugin/taxonomy-terms-order": "^1.9",
      "wpackagist-plugin/woo-variation-gallery": "1.3.28",
      "wpackagist-plugin/relevanssi": "4.26.0",
      "wpackagist-plugin/google-site-kit":"1.168.0"
    },
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "allow-plugins": {
        "johnpbloch/wordpress-core-installer": true,
        "composer/installers": true
      }
    },
    "require-dev": {
      "squizlabs/php_codesniffer": "^3.7.1",
      "roave/security-advisories": "dev-latest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
      "installer-paths": {
        "wp-content/mu-plugins/{$name}/": [
          "type:wordpress-muplugin"
        ],
        "wp-content/plugins/{$name}/": [
          "type:wordpress-plugin"
        ],
        "wp-content/themes/{$name}/": [
          "type:wordpress-theme"
        ]
      },
      "wordpress-install-dir": "wp"
    },
    "scripts": {
      "test": [
        "phpcs"
      ]
    }
  }